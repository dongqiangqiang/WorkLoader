vmstat.command=vmstat -n
vmstat.startSkip=2
vmstat.datagroup=null
vmstat.datagroup.split=\ 
vmstat.datagroup.count=1
vmstat.datagroup.head=run,block,swap,free,buffer,cache,si,so,bi,bo,in,cs,us,sy,id,wa
vmstat.chart.list=proc,memory,swap,io,system,cpu
vmstat.chart.proc=yname:number;line:run,block
vmstat.chart.memory=yname:byte;stack:swap,free,buffer,cache
vmstat.chart.swap=yname:byte;line:si,so
vmstat.chart.io=yname:byte;line:bi,bo
vmstat.chart.system=yname:number;line:in,cs
vmstat.chart.cpu=yname:percentage;stack:us,sy,id,wa



dstat.command=dstat --mem --io --cpu --net -N eth0,eth1,total --disk --output
dstat.startSkip=7
dstat.datagroup=null
dstat.datagroup.split=,
dstat.datagroup.count=1
dstat.datagroup.head=used,buffer,cached,free,ioread,iowrit,usr,idl,wai,hiq,siq,eth0recv,eth0send,eth1recv,eth1send,totalrecv,totalsend,diskread,diskwrit
dstat.chart.list=memory,io,cpu,network0,network1,networkt,disk
dstat.chart.memory=yname:percentage;stack:used,buffer,cached,free
dstat.chart.io=yname:byte;line:ioread,iowrit
dstat.chart.cpu=yname:percentage;stack:usr,idl,wai,hiq,siq
dstat.chart.network0=yname:byte;line:eth0recv,eth0send
dstat.chart.network1=yname:byte;line:eth1recv,eth1send
dstat.chart.networkt=yname:byte;line:totalrecv,totalsend
dstat.chart.disk=yname:byte;line:diskread,diskwrit

iostat.command=iostat 
iostat.startSkip=1
iostat.datagroup=[cpu,startWith:avg-cpu][device,startWith:Device]
iostat.datagroup.cpu.head=user,nice,system,iowait,steal,idle
iostat.datagroup.cpu.split=\ 
iostat.datagroup.cpu.count=3
iostat.datagroup.device.head=device,tps,kB_read/s,kB_wrtn/s,kB_read,kB_wrtn
iostat.datagroup.device.split=\ 
iostat.datagroup.device.count=10
iostat.chart.list=cpu,devicesdb,devicesdd,devicesdc,devicesde,devicesda,tps
iostat.chart.cpu=yname:byte;group:cpu;select cpu:;stack:user,nice,system,iowait,steal,idle
iostat.chart.devicesdb=yname:kilobyte;group:device;select device:sdb;line:kB_read/s,kB_wrtn/s,kB_read,kB_wrtn
iostat.chart.devicesdd=yname:kilobyte;group:device;select device:sdd;line:kB_read/s,kB_wrtn/s,kB_read,kB_wrtn
iostat.chart.devicesdc=yname:kilobyte;group:device;select device:sdc;line:kB_read/s,kB_wrtn/s,kB_read,kB_wrtn
iostat.chart.devicesde=yname:kilobyte;group:device;select device:sde;line:kB_read/s,kB_wrtn/s,kB_read,kB_wrtn
iostat.chart.devicesda=yname:kilobyte;group:device;select device:sda;line:kB_read/s,kB_wrtn/s,kB_read,kB_wrtn
iostat.chart.tps=yname:byte;group:device;aggregate device;line:tps

sar.command=sar -P ALL
sar.startSkip=1
sar.datagroup=[cpu,indexOf:CPU]
sar.datagroup.cpu.split=\ 
sar.datagroup.cpu.count=18
sar.datagroup.cpu.head=time,timeflag,cpu,user,nice,system,iowait,steal,idle
sar.chart.list=cpuall,cpu0,user
sar.chart.cpuall=yname:percentage;group:cpu;select cpu:all;line:user,nice,system,iowait,steal,idle
sar.chart.cpu0=yname:percentage;group:cpu;select cpu:0;line:user,nice,system,iowait,steal,idle
sar.chart.user=yname:percentage;group:cpu;aggregate cpu;line:user

